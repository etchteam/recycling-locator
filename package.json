{
  "name": "@etchteam/recycling-locator",
  "version": "1.69.0",
  "description": "Find places to recycle, refill or reuse items.",
  "type": "module",
  "author": "Etch <hey@etch.co>",
  "license": "MIT",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etchteam/recycling-locator.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:uat": "./scripts/build-uat.sh",
    "build:css": "postcss src/styles/base.css -o public/styles.css",
    "build": "npm run build:css && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx,yml,yaml,json}' && stylelint 'src/**/*.{css,scss}'",
    "prepare": "node .husky/install.mjs",
    "prestart": "npm run build",
    "release": "semantic-release",
    "start:css": "npm run build:css -- --watch",
    "start:vite": "vite --host",
    "start": "run-p start:css start:vite",
    "storybook:build": "storybook build",
    "storybook:start": "storybook dev -p 6006",
    "storybook": "run-p start:css storybook:start",
    "test:unit": "vitest run",
    "test:unit:ui": "vitest --ui",
    "test:end-to-end": "playwright test",
    "test:end-to-end:debug": "playwright test --debug",
    "test:end-to-end:ui": "playwright test --ui",
    "test": "npm run test:unit && npm run test:end-to-end"
  },
  "dependencies": {
    "@etchteam/diamond-ui": "^1.37.2",
    "@headlessui/react": "^2.2.4",
    "@here/maps-api-for-javascript": "^1.68.0",
    "@preact/signals": "^2.0.4",
    "@sentry/browser": "^9.22.0",
    "axios": "^1.11.0",
    "axios-cache-interceptor": "^1.8.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "fetch-jsonp": "^1.3.0",
    "i18next": "^25.2.0",
    "i18next-http-backend": "^3.0.2",
    "linkify-react": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "lodash": "^4.17.21",
    "nl2br": "^0.0.3",
    "preact": "^10.26.6",
    "preact-custom-element": "^4.3.0",
    "react-i18next": "^15.5.2",
    "wouter-preact": "^3.8.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@etchteam/eslint-config": "^2.2.16",
    "@etchteam/stylelint-config": "^1.6.0",
    "@playwright/test": "^1.51.1",
    "@preact/preset-vite": "^2.8.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.3",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@sentry/vite-plugin": "^2.16.0",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/preact": "^7.6.11",
    "@storybook/preact-vite": "^8.6.4",
    "@types/fetch-jsonp": "^1.0.0",
    "@vitest/ui": "^3.1.1",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "happy-dom": "^20.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "modify-selectors": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.49.1",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-minify": "^1.1.0",
    "postcss-nesting": "^12.1.2",
    "preact-shadow-dom": "^1.0.1",
    "prettier": "^3.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "semantic-release": "^23.1.1",
    "storybook": "^8.6.4",
    "stylelint": "^15.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.2.1",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "npm": "^9 || ^10",
    "node": "^18 || ^20 || ^22"
  }
}
